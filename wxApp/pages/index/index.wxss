view {
  /* 使用主题颜色 */
  /* color: var(--themeColor) */
}
.index_swiper swiper {
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_nav {
  display: flex;
}
.index_nav navigator {
  flex: 1;
  padding: 20rpx;
}
.index_nav navigator image {
  width: 100%;
}
.index_floot .floot_group .groups_title {
  padding: 8rpx 0;
  width: 100%;
}
.index_floot .floot_group .groups_content {
  overflow: hidden;
}
.index_floot .floot_group .groups_content navigator {
  float: left;
  width: 33.33%;
}
.index_floot .floot_group .groups_content navigator image {
  width: 100%;
}
.index_floot .floot_group .groups_content navigator:nth-last-of-type(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index_floot .floot_group .groups_content navigator:nth-last-of-type(-n+4) image {
  height: 100%;
}
.index_floot .floot_group .groups_content navigator:nth-last-of-type(-n+2) {
  border-top: 10rpx solid #fff;
}
